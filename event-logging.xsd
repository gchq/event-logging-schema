                            <xs:documentation>The HTTP method, e.g. GET, POST, DELETE, PUT etc</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="HTTPVersion" type="xs:string" minOccurs="0" maxOccurs="1">
