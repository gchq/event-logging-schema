<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           id="permission-attribute-simple-type-v4.0.2"
           targetNamespace="http://event-logging/permission-attribute-simple-type"
           version="4.0.2">
   <xs:simpleType name="PermissionAttributeSimpleType">
      <xs:annotation>
         <xs:documentation>The types of permission that can be assigned to an entity such as a document.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Author"/>
         <xs:enumeration value="Owner"/>
         <xs:enumeration value="Read"/>
         <xs:enumeration value="Write"/>
         <xs:enumeration value="Execute"/>
      </xs:restriction>
   </xs:simpleType>
</xs:schema>
 