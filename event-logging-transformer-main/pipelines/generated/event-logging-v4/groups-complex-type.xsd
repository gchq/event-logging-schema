<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:GroupComplexType="http://event-logging/group-complex-type"
           elementFormDefault="qualified"
           id="groups-complex-type-v4.0.2"
           targetNamespace="http://event-logging/groups-complex-type"
           version="4.0.2">
   <xs:import namespace="http://event-logging/group-complex-type"
              schemaLocation="group-complex-type.xsd"/>
   <xs:complexType name="GroupsComplexType">
      <xs:annotation>
         <xs:documentation>Describes a collection of group entities, e.g. user groups or access control groups.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element maxOccurs="unbounded"
                     minOccurs="0"
                     name="Group"
                     type="GroupComplexType:GroupComplexType">
            <xs:annotation>
               <xs:documentation>One or more group entities, e.g. user groups or access control groups.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
</xs:schema>
 